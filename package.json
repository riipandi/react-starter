{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css:build": "node_modules/.bin/postcss src/css/index.src.css -o src/css/index.css",
    "css:watch": "node_modules/.bin/postcss src/css/index.src.css -o src/css/index.css -w",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "serve -s build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.3",
    "postcss-cli": "^6.1.0",
    "tailwindcss": "^0.7.3"
  }
}
