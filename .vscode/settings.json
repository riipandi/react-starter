{
  "files.exclude": {
    "**/.git": true,
    "**/.tmb": true,
    "**/.DS_Store": true,
    "**/.quarantine": true,
    "**/.circleci": true,
    "**/.babelrc": true,
    "**/.editorconfig": true,
    "**/.gitattributes": true,
    "**/.gitignore": false,
    "**/.gitlab-ci.yml": true,
    "**/.vsts-ci.yml": true,
    "**/tslint.json": true,
    "**/tsconfig.json": true,
    "**/package-lock.json": true,
    "**/tests-jest.xml": true,
    "**/yarn.lock": true,
    "**/yarn-error.log": true,
    "**/docker/": true,
    "**/node_modules": true,
    "**/tests": false,
    "**/*.md": false,
  },
  "files.trimTrailingWhitespace": true,
  "eslint.alwaysShowStatus": true,
  "eslint.enable": true,
  "eslint.runtime": null,
}
